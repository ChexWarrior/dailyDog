{
  "manifest_version": 2,
  "name": "Daily Dog",
  "version": "1.0",
  "homepage_url": "https://github.com/ChexWarrior/dailyDog",
  "description": "Shows a picture of a cute dog with a quote on a regular interval.",
  "permissions": [
    "*://*/*",
    "alarms",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
